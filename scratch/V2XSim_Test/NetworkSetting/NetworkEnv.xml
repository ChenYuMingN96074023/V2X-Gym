<NetworkEnviroment> <!--Setting Network Enviroment in this section-->
    
    <epcHelper InstanceType="CreateObject"  Class="PointToPointEpcHelper" > <!--Ptr<type> name = CreateObject<type> ();-->
    </epcHelper>

    <ltehelper InstanceType="CreateObject" Clas="ltehelper"> <!--Ptr<PointToPointEpcHelper> epcHelper = CreateObject<PointToPointEpcHelper> ();-->
        <CreateObjectWithAttributes> &quot;MinX&quot;, DoubleValue (-100.0), &quot;MinY&quot;, DoubleValue (-100.0), &quot;DeltaX&quot;, DoubleValue (5.0), &quot;DeltaY&quot;, DoubleValue (20.0)</CreateObjectWithAttributes>
        <SetEpcHelper> &quote;epcHelper&quote; </SetEpcHelper> <!-- ltehelper->SetEpcHelper("epcHelper");    --> 
        
        <SetAttribute>&quot;LteSpectrumPhy::CtrlErrorModelEnabled&quit;, BooleanValue (false)</setattribute>
        <EnablePhyTraces></EnablePhyTraces>
    </ltehelper>

    <internet InstanceType="InternetStackHelper" > 
        <Install>remoteHostContainer</Install>
    </internet>
        

    <p2ph InstanceType="PointToPointHelper" >
        <SetDeviceAttribute>&quot;DataRate&quot;, DataRateValue (DataRate (&quot;100Gb/s&quot;))</SetDeviceAttribute>
    </p2ph>

    <Config::SetDefault InstanceType="FunctionCall" arguments="&quot;ns3::LteSpectrumPhy::CtrlErrorModelEnabled&quit;, BooleanValue (false)"> <!--Config::SetDefault(Arguments)-->
    </Config::SetDefault>

    <EpsBear type="NGBR_VIDEO_TCP_DEFAULT"> <!--vehicles bearer type -->
        

